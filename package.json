{
  "name": "parcel-bug-report-examples",
  "version": "1.0.0",
  "description": "Collection of examples for parcel bug reports",
  "scripts": {
    "clean": "rm -rf .parcel-cache",
    "prebuild": "npm run clean",
    "build": "parcel build src/index.html",
    "prebuild:simple": "npm run clean",
    "build:simple": "npm run build -- --no-scope-hoist",
    "start": "npm run build && npm run serve",
    "start:simple": "npm run build:simple && npm run serve",
    "serve": "serve dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garthenweb/parcel-bug-report-examples.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garthenweb/parcel-bug-report-examples/issues"
  },
  "homepage": "https://github.com/garthenweb/parcel-bug-report-examples#readme",
  "devDependencies": {
    "parcel": "^2.0.0-nightly.163",
    "serve": "^11.3.0"
  }
}
